{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "outDir": "build",
    "rootDir": "src",
    "declaration": false,
    "noImplicitAny": true,
    "typeRoots": ["./node_modules/@types"],
    "skipLibCheck": true
  },
  "exclude": ["Build/*", "./node_modules/*"],
  "include": ["src/**/*.ts"]
}
